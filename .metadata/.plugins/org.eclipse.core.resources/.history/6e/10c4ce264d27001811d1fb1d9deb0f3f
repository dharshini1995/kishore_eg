package springdemo;

import java.util.List;

public class Restaurent {
	
	public void greeting()
	{
		System.out.println("Welcome dear custome his is lifecycle demo" );
	}
	
	@PostConstruct
	public void init()
	{
		System.out.println("Restauren bean is going through init");
	}
	
	@PreDestroy
	public void destroy()
	{
		System.out.println("Bean will be destroyed now");
	}
}
